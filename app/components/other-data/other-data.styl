#other_data
  .last_data
    color steelblue
    text-align center
    padding 5px 0
  .gender
    width 100%
    padding 10px 80px
    .circle-wrap
      width 50%
      display inline-block
      .centered
        text-align center
        h1
          display inline-block
          position relative
          top -9px
          left -4px
        .circle
          display inline-block
          width 30px
          height 30px
          border-radius 50%
          border 1px solid gray
          padding 2px
          .bcg
            background-color deeppink
            border-radius 50%
            width 24px
            height 24px
  .inputs
    width 90%
    margin 0 auto
    position relative
    input
      width 100%
      height 50px
      margin 5px 0
      border-radius 10px
      border-top 1px solid grey
      border-left 1px solid grey
      border-right 2px solid grey
      border-bottom 2px solid grey
      padding 5px
  .select-wrap
    width 100%
    height 50px
    margin 5px 0
    border-radius 10px
    border-top 1px solid grey
    border-left 1px solid grey
    border-right 2px solid grey
    border-bottom 2px solid grey
    padding 8px 0px
    .active
      border none
      color grey
  .input-wrap
    width 100%
    height 40px
    padding 0 10px
    .label
      width 45%
      display inline-block
      font-size 21px
      margin 0 9px
    input
      display inline-block
      width 45%
      height 30px
      border 1px solid grey
      text-align end
      color #00bfff
      padding 0 5px
      &:focus
        outline none
    .clients-list-wrap-right
      right 0
    .clients-list-wrap-left
      left 0
    .clients-list-wrap
      margin 0 31px
      width 43%
      background-color white
      z-index 15
      position absolute
      border 1px solid silver
      max-height 200px
      overflow-y scroll
      overflow-x hidden
      div
        font-size 21px
        text-align end
        padding 0 5px
  .btn-wrap
    width 100%
    padding-top 20px
    button
      display block
      margin 0 auto
      width 50%
      height 40px
      border-radius 20px
      border 2px solid grey
      background-color white
      a
        color #6b6b6b
        text-decoration none
