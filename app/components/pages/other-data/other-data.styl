#other_data
  .last_data
    color steelblue
    text-align center
    padding 5px 0
  .gender
    width 100%
    padding 10px 80px
    .circle-wrap
      width 50%
      display inline-block
      .centered
        text-align center
        h1
          display inline-block
          position relative
          top -9px
          left -4px
        .circle
          display inline-block
          width 30px
          height 30px
          border-radius 50%
          border 1px solid gray
          padding 2px
          .bcg
            background-color deeppink
            border-radius 50%
            width 24px
            height 24px
  .inputs
    width 90%
    margin 0 auto
    position relative
    .field
      width 100%
      height 50px
      margin 5px 0
      border-radius 10px
      border-top 1px solid grey
      border-left 1px solid grey
      border-right 2px solid grey
      border-bottom 2px solid grey
      padding 5px
      display flex
      align-items center
  .select-wrap
    width 100%
    height 50px
    margin 5px 0
    border-radius 10px
    border-top 1px solid grey
    border-left 1px solid grey
    border-right 2px solid grey
    border-bottom 2px solid grey
    padding 8px 0px
    .active
      border none
    .options
      width 100.5%
      top 43px
      padding 0 5px
      border-radius 10px
      border-top 1px solid grey
      border-left 1px solid grey
      border-right 2px solid grey
      border-bottom 2px solid grey
  .input-wrap
    width 100%
    .label
      width 43%
      display inline-block
      font-size 18px
      color grey
    input
      display inline-block
      width 57%
      height 30px
      border-top 1px solid grey
      border-left 1px solid grey
      border-right 2px solid grey
      border-bottom 2px solid grey
      text-align start
      color #00bfff
      padding 0 5px
      border-radius 5px
      margin-bottom 2px
      &:focus
        outline none
    .clients-list-wrap-right
      right 0
    .clients-list-wrap-left
      left 0
    .clients-list-wrap
      width 57%
      background-color white
      z-index 15
      border-radius 5px
      position absolute
      border-top 1px solid grey
      border-left 1px solid grey
      border-right 2px solid grey
      border-bottom 2px solid grey
      max-height 200px
      overflow-y scroll
      overflow-x hidden
      div
        font-size 21px
        text-align start
        padding 0 5px
  .btn-wrap
    width 100%
    padding-top 20px
    button
      display block
      margin 0 auto
      width 50%
      height 40px
      border-radius 20px
      border 2px solid grey
      background-color white
      color #6b6b6b
  .checkbox_container
    width 100%
    padding 5px 0
    border 1px solid white
    .checkbox_wrap
      display inline-block
      width 10%
      .checkbox
        position relative
        top 1px
        width 25px
        height 25px
    .label_wrap
      display inline-block
      width 80%
      .checkbox_label
        font-size 12px
